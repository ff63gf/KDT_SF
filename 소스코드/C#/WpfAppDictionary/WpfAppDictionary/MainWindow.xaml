        <TextBlock HorizontalAlignment="Left" Margin="325,131,0,0" TextWrapping="Wrap" Text="ID" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="325,196,0,0" TextWrapping="Wrap" Text="PW" VerticalAlignment="Top"/>
        <Button x:Name="btnLogin" Content="Login" HorizontalAlignment="Left" Height="27" Margin="361,273,0,0" VerticalAlignment="Top" Width="69" Click="btnLogin_Click"/>

    </Grid>
</Window>
