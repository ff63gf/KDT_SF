<Window x:Class="WpfAppCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppCalculator"
        mc:Ignorable="d"
        Title="MainWindow" Height="509" Width="338">
    <Grid>
        <Button x:Name="button0" Content="0" HorizontalAlignment="Left" Height="46" Margin="108,399,0,0" VerticalAlignment="Top" Width="61" Click="button0_Click"/>
        <Button x:Name="button1" Content="1" HorizontalAlignment="Left" Height="46" Margin="42,345,0,0" VerticalAlignment="Top" Width="61" Click="button1_Click"/>
        <Button x:Name="button2" Content="2" HorizontalAlignment="Left" Height="46" Margin="108,345,0,0" VerticalAlignment="Top" Width="61" Click="button2_Click"/>
        <Button x:Name="button0복사__C_2" Content="0" HorizontalAlignment="Left" Height="46" Margin="174,345,0,0" VerticalAlignment="Top" Width="60"/>
        <Button x:Name="button4" Content="4" HorizontalAlignment="Left" Height="46" Margin="42,294,0,0" VerticalAlignment="Top" Width="61" Click="button4_Click"/>
        <Button x:Name="button5" Content="5" HorizontalAlignment="Left" Height="46" Margin="108,294,0,0" VerticalAlignment="Top" Width="61" Click="button5_Click"/>
        <Button x:Name="button0복사__C_2복사__C_" Content="0" HorizontalAlignment="Left" Height="46" Margin="174,294,0,0" VerticalAlignment="Top" Width="60"/>
        <Button x:Name="button7" Content="7" HorizontalAlignment="Left" Height="46" Margin="42,243,0,0" VerticalAlignment="Top" Width="61" Click="button7_Click"/>
        <Button x:Name="button8" Content="8" HorizontalAlignment="Left" Height="46" Margin="108,243,0,0" VerticalAlignment="Top" Width="61" Click="button8_Click"/>
        <Button x:Name="button0복사__C_2복사__C_1" Content="0" HorizontalAlignment="Left" Height="46" Margin="174,243,0,0" VerticalAlignment="Top" Width="60"/>
        <Button x:Name="button3" Content="3" HorizontalAlignment="Left" Height="46" Margin="174,345,0,0" VerticalAlignment="Top" Width="60" Click="button3_Click"/>
        <Button x:Name="button6" Content="6" HorizontalAlignment="Left" Height="46" Margin="174,294,0,0" VerticalAlignment="Top" Width="60" Click="button6_Click"/>
        <Button x:Name="button9" Content="9" HorizontalAlignment="Left" Height="46" Margin="174,243,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="button9_Click"/>
        <Button x:Name="buttonEqual" Content="=" HorizontalAlignment="Left" Height="46" Margin="239,396,0,0" VerticalAlignment="Top" Width="60" Click="buttonEqual_Click"/>
        <Button x:Name="buttonPlus" Content="+" HorizontalAlignment="Left" Height="46" Margin="239,345,0,0" VerticalAlignment="Top" Width="60" Click="buttonPlus_Click"/>
        <Button x:Name="buttonMinus" Content="-" HorizontalAlignment="Left" Height="46" Margin="239,294,0,0" VerticalAlignment="Top" Width="60" Click="buttonMinus_Click"/>
        <Button x:Name="buttonMultiply" Content="*" HorizontalAlignment="Left" Height="46" Margin="239,243,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonMultiply_Click"/>
        <Button x:Name="buttonSign" Content="+/-" HorizontalAlignment="Left" Height="46" Margin="42,399,0,0" VerticalAlignment="Top" Width="61" Click="buttonSign_Click"/>
        <Button x:Name="buttonPercent" Content="%" HorizontalAlignment="Left" Height="46" Margin="174,192,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonPercent_Click"/>
        <Button x:Name="buttonDivide" Content="/" HorizontalAlignment="Left" Height="46" Margin="239,192,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonDivide_Click"/>
        <Button x:Name="buttonClear" Content="C" HorizontalAlignment="Left" Height="46" Margin="174,141,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonClear_Click"/>
        <Button x:Name="buttonBack" Content="&lt;" HorizontalAlignment="Left" Height="46" Margin="239,141,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonBack_Click"/>
        <Button x:Name="buttonHistory" Content="History" HorizontalAlignment="Left" Height="46" Margin="109,141,0,0" VerticalAlignment="Top" Width="60" RenderTransformOrigin="1.772,-0.783" Click="buttonHistory_Click"/>
        <TextBox x:Name="labelNumber" HorizontalAlignment="Left" Margin="174,92,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Width="127" IsReadOnly="True" BorderBrush="{x:Null}" HorizontalContentAlignment="Right" Height="28" TextChanged="labelNumber_TextChanged"/>
        <TextBox x:Name="labelResult" HorizontalAlignment="Left" Margin="172,64,0,0" TextWrapping="Wrap" Text="결과값" VerticalAlignment="Top" Width="127" IsReadOnly="True" BorderBrush="{x:Null}" HorizontalContentAlignment="Right" Height="28"/>

    </Grid>
</Window>
